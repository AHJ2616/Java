package common;

public class Person {
	// JavaBeans 규약
	
	private String name;
	private int age;
	
	//기본 생성자
	public Person() {}
	
	
	// getter&setter method
	
	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setAge(int age) {
		this.age = age;
	}
}//class end
