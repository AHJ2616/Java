package utils;

import javax.servlet.jsp.JspWriter;

public class JSFunction { //프론트에서 경고창(alert)을 띄우는 코드

	
	public void alertLocation(String msg,String url,JspWriter out) {
		try {
			String script = "" //삽입할 자바 스크립트 코드
						  + "<script>"
						  +"alert('"+msg+"'")";"
						  + "location.hredf='"+url+"';"
						  +"</script>";"
		
		}
		
	}//method end
	
	
}//class end
